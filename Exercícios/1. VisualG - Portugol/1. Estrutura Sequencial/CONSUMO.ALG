Algoritmo "consumo"

Var

      distancia, combustivel, consumomedio : real

Inicio
   escreva("Distância percorrida: ")
   leia(distancia)
   escreva("Combustível gasto: ")
   leia(combustivel)
   
   consumomedio <- distancia / combustivel
   
   escreva("Consumo médio: ", consumomedio:5:3)


Fimalgoritmo